{
  "scripts": {
    "build": "npm-run-all bootstrap  fonts css-minify",
    "bootstrap": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules custom.scss build/sidebar-bootstrap.min.css",
    "fonts": "./gfontsdw.js 'https://fonts.googleapis.com/css2?family=Montserrat:wght@300%3B400%3B700&display=swap' build",
    "css-minify": "cleancss -O1 --source-map -o build/bootstrapsidebar-fonts.min.css build/bootstrapsidebar-fonts.css"
  },
  "dependencies": {
    "bootstrap": "5.3.8",
    "clean-css-cli": "~5.6",
    "fs": "latest",
    "https": "latest",
    "npm-run-all": "latest",
    "path": "path",
    "sass": "1.78.0"
  }
}
